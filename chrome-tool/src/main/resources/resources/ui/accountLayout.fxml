<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="331.0" prefWidth="334.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.guuidea.component.chrome.tool.ui.GoogleAccountController">
    <children>
        <Label layoutX="43.0" layoutY="39.0" prefHeight="26.0" prefWidth="63.0" text="%G_USERNAME"/>
        <TextField fx:id="gUserName" layoutX="106.0" layoutY="39.0"/>
        <Label layoutX="43.0" layoutY="87.0" prefHeight="26.0" prefWidth="63.0" text="%G_PASSWORD"/>
        <PasswordField fx:id="gPassword" layoutX="106.0" layoutY="87.0"/>
        <Label layoutX="43.0" layoutY="220.0" prefHeight="26.0" prefWidth="63.0" text="%G_CHECK_IP"/>
        <CheckBox fx:id="gCheckIp" layoutX="107.0" layoutY="220.0" mnemonicParsing="false" prefHeight="26.0"
                  prefWidth="167.0" text="%G_CHECK_IP_DESC"/>
        <Label layoutX="43.0" layoutY="168.0" prefHeight="27.0" prefWidth="63.0" text="%G_PROXY_SELECT"/>
        <ComboBox fx:id="gProxySelect" layoutX="106.0" layoutY="168.0" prefHeight="27.0" prefWidth="167.0"/>
        <Button layoutX="150.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleSubmit"
                text="%G_CONFIRM_SUBMIT"/>
        <Button layoutX="229.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleCancel" text="%G_CANCEL"/>
        <Label layoutX="43.0" layoutY="128.0" prefHeight="26.0" prefWidth="63.0" text="%G_LOGIN_URL"/>
        <TextField fx:id="gLoginUrl" layoutX="107.0" layoutY="128.0"/>
    </children>
</AnchorPane>
